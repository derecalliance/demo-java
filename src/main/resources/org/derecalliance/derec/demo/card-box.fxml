<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.derecalliance.derec.demo.CardBoxController">
    <Label fx:id="cardLabel" text="Default Text"/>
    <HBox>
        <Region HBox.hgrow="ALWAYS"></Region>
        <Button fx:id="cardButton" style="-fx-background-radius: 10px;">
            <graphic>
                <ImageView fx:id="cardImage" fitWidth="20" fitHeight="20"/>
            </graphic>
        </Button>
    </HBox>
</VBox>

